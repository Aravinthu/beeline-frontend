    "uuid": "^2.0.3",
    "webpack": "^2.4.1"
  },
  "devDependencies": {
    "inline-environment-variables-webpack-plugin": "^1.2.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard",
    "cordova-plugin-inappbrowser",
    "cordova-plugin-app-version",
    "cordova-plugin-geolocation",
    "cordova-plugin-network-information",
    "cordova-hot-code-push-plugin",
    {
      "locator": "https://github.com/EddyVerbruggen/SocialSharing-PhoneGap-Plugin.git",
      "id": "cordova-plugin-x-socialsharing"
    },
    "cordova-universal-links-plugin"
  ],
  "cordovaPlatforms": [
    "ios",
    "android"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "webpack",
    "clean": "ionic platform remove android; ionic platform remove ios; ionic platform remove browser; git checkout package.json",
    "clean-deploy": "shx rm -rf node_modules/gh-pages/.cache",
    "stg-deploy": "npm run stg-cp && npm run stg-build && npm run stg-hcp && npm run stg-gh-pages",
    "stg-gh-pages": "npm run clean-deploy && gh-pages -d build-stg",
    "stg-build": " cross-env BACKEND_URL=https\\://api.beeline.sg BUILD_PREFIX=build-stg webpack",
    "stg-cp": "shx mkdir build-stg && shx cp -r 'www/*' build-stg",
    "stg-hcp": "node scripts/hcp.js app-staging.beeline.sg build-stg",
    "deploy": "node scripts/warn.js && npm run live-cp && npm run live-build && npm run live-hcp && npm run live-gh-pages",
    "live-gh-pages": "npm run clean-deploy &&  gh-pages -r https://github.com/datagovsg/beeline-frontend-deploy.git -d build",
    "live-build": " cross-env BACKEND_URL=https\\://api.beeline.sg BUILD_PREFIX=build webpack",
    "live-cp": "shx mkdir build && shx cp -r 'www/*' build",
    "live-hcp": "node scripts/hcp.js app.beeline.sg build",
    "build": "cross-env BACKEND_URL=https\\://api.beeline.sg webpack",
    "dev": "cross-env BACKEND_URL=https\\://api.beeline.sg webpack -w",
    "start": "http-server -p 5000 -c-1 www"
  },
