{
  "name": "beeline2",
  "version": "1.1.1",
  "description": "beeline2: An Ionic project",
  "engines": {
    "node": "7.8.x"
  },
  "dependencies": {
    "angular-google-maps": "^2.4.1",
    "angular-simple-logger": "^0.1.7",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
