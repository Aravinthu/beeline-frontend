    "commonmark": "^0.26.0",
    "compare-versions": "^3.0.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.6.0",
    "eslint-config-google": "^0.6.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "gh-pages": "^0.12.0",
    "html-loader": "^0.4.4",
    "http-server": "^0.9.0",
    "imports-loader": "^0.7.1",
    "json-loader": "^0.5.4",
    "left-pad": "^1.1.3",
    "lodash": "^4.16.2",
    "moment": "^2.15.1",
    "multiple-date-picker": "git+https://github.com/datagovsg/MultipleDatePicker.git#annotations",
    "ngclipboard": "^1.1.1",
    "node-sass": "^4.5.2",
    "q": "^1.4.1",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.4",
